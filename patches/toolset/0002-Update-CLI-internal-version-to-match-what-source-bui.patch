From aca042b7c8f85b1bd46981a4f51229a6b25c3733 Mon Sep 17 00:00:00 2001
From: Chris Rummel <crummel@microsoft.com>
Date: Tue, 17 Jul 2018 12:08:25 -0500
Subject: [PATCH] Update CLI internal version to match what source-build
 produces.

See https://github.com/dotnet/source-build/issues/657 for details.  This
patch can be removed after that issue is fixed.
---
 build/DependencyVersions.props | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/build/DependencyVersions.props b/build/DependencyVersions.props
index e5e1c482f..bf17f6d01 100644
--- a/build/DependencyVersions.props
+++ b/build/DependencyVersions.props
@@ -2,7 +2,7 @@
 <Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
   <PropertyGroup>
     <DotNetCoreSdkLKGVersion>3.0.100-preview-009722</DotNetCoreSdkLKGVersion>
-    <DotnetCliInternalVersion>3.0.100-preview.18578.3</DotnetCliInternalVersion>
+    <DotnetCliInternalVersion>3.0.100-alpha1-00000001-01</DotnetCliInternalVersion>
     <MicrosoftNETSdkPackageVersion>3.0.100-preview.18578.2</MicrosoftNETSdkPackageVersion>
     <MicrosoftNETBuildExtensionsPackageVersion>$(MicrosoftNETSdkPackageVersion)</MicrosoftNETBuildExtensionsPackageVersion>
     <MicrosoftBuildPackageVersion>16.0.0-preview.225</MicrosoftBuildPackageVersion>
